{
  "manifest_version": 3,
  "name": "Additional Miniplayer",
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "version": "0.0.0",
  "description": "My Chrome Extension",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Additional Miniplayer",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}